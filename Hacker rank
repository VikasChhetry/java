/*import java.util.*;
public class homework{
    public static int calculationSum(int a, int b){
        int sum =a+b;
        return sum;
    }
    public static int claculationMulti(int a , int b){
        return a*b;
    //     int multi= a*b;//>>>>WE CAN DIRECTLY WRITE RETURN A*B;
    //     return multi;
    }
    public static void newr(int a){
        int fact = 1;
        for ( int i=a; i>=1;i--){
            fact= fact*i;
            // System.out.println(fact);
            

        }
        System.out.println(fact);
        return;
    }
    public static void main ( String[]args){
        Scanner sc = new Scanner (System.in);
